{
  "name": "@push-manager/server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "TZ=UTC ts-node-dev --respawn --clear --transpile-only src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "generate:models": "ts-node src/utils/generateModels.util.ts",
    "db:generate": "ts-node -r dotenv/config src/db/run-drizzle.ts generate",
    "db:push": "ts-node -r dotenv/config src/db/run-drizzle.ts push",
    "db:pull": "ts-node -r dotenv/config src/db/run-drizzle.ts pull",
    "db:studio": "ts-node -r dotenv/config src/db/run-drizzle.ts studio",
    "db:migrate": "ts-node -r dotenv/config src/db/run-drizzle.ts migrate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@bull-board/express": "^6.7.10",
    "@noble/ciphers": "^1.2.1",
    "@noble/hashes": "^1.7.1",
    "@push-manager/shared": "1.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node-cron": "^3.0.11",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.40.0",
    "express": "^4.21.2",
    "firebase-admin": "^13.1.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "node-apn": "^3.0.0",
    "node-cron": "^4.0.6",
    "oracledb": "^6.7.1",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "sequelize": "^6.37.5",
    "spacetime": "^7.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.5.4",
    "@types/oracledb": "^6.5.1",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.30.5",
    "sequelize-auto": "^0.8.8",
    "ts-node-dev": "^2.0.0"
  }
}
